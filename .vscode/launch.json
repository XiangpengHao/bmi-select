	"version": "0.2.0",
	"configurations": [
		{
			"type": "lldb",
			"request": "launch",
			"name": "Debug benchmark 'bit_pack_bench'",
			"cargo": {
				"args": [
					"test",
					"--no-run",
					"--bench=bit_pack_bench",
					"--package=bmi-select"
				],
				"filter": {
					"name": "bit_pack_bench",
					"kind": "bench"
				}
			},
			"args": [
				"bit_unpack"
			],
			"cwd": "${workspaceFolder}"
		}
	]
}